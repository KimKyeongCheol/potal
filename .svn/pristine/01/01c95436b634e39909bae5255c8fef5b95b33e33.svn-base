@charset "utf-8";
/* default 1280px */
html{font-size:20px;width:100%;;box-sizing:border-box}
body{font-size:1rem;width:100%;box-sizing:border-box}
#wrap{position:relative;width:100%;max-width:100%;box-sizing:border-box}

.base-inner{width:64rem;position:relative;margin:0 auto;max-width:100%}
.clear:after{content:'';display:block;clear:both}
.mb{display:none}
.pc{display:block}

 
@media screen and (max-width: 1280px){ 
	.w-base{width:100%}
 	#container .content{width:80%;padding-right:1rem;box-sizing:border-box}
	#container .content.long{width:100%;float:none;padding:0}
	.mb{display:block}
	.pc{display:none}
}



.hd-top-wrap{height:5.1rem;position:relative;box-sizing:border-box;padding-top:1.15rem;}
.hd-top-wrap .hd-logo-box{position:absolute;left:0;top:0}
.hd-top-wrap .hd-logo-box h1{font-size:0;background:url('../images/hd-logo.png') center center no-repeat;width:8.75rem;height:3.05rem;}
.hd-top-wrap .hd-search-box{width:29rem;height:3rem;padding-right:3rem;margin:0 auto;display:block;box-sizing:border-box;border:4px solid #0c6fb6;position:relative}
.hd-top-wrap .hd-search-box .input{width:100%;height:2.5rem;line-height:2.5rem;border:none;box-sizing:border-box;padding:0 1rem}
.hd-top-wrap .hd-search-box .ico-search{background:url('../images/ico-search.png') center center no-repeat;width:2.8rem;height:2.5rem;position:absolute;right:0;top:0;display:block;font-size:0}
.hd-top-wrap .hd-util-box{position:absolute;right:0;top:0;padding-top:.7rem}
.hd-top-wrap .hd-util-box li{float:left}
.hd-top-wrap .hd-util-box li a{display:block;height:1.6rem;line-height:1.6rem;font-size:.8rem;color:#020202;padding:0 1.25rem}
.hd-top-wrap .hd-util-box li.on a{background:#0c6fb6;color:#fff}

.gnb-wrap{height:5.16rem;position:relative;z-index:99;background:url('../images/hd-bg-02.png') left top repeat-x}
.gnb-wrap .gnb-menu-list-box {margin-left:-2.2rem;position:relative;}
.gnb-wrap .gnb-menu-list-box > li{float:left;box-sizing:border-box;position:relative}
.gnb-wrap .gnb-menu-list-box > li > a{display:inline-block;line-height:5.15rem;font-size:1.1rem;color:#353535;padding:0 2.2rem;}

.gnb-wrap .gnb-menu-list-box > li > div{height:0;position:absolute;left:0;top:5.16rem;right:0;overflow:hidden;}
.gnb-wrap .gnb-menu-list-box > li.on > div{background:#0c6fb6}
.gnb-wrap .gnb-menu-list-box > li.on:before{content:'';display:block;position:absolute;left:0;top:0;right:0;background:#0c6fb6;height:3px;width:100%}
.gnb-wrap .gnb-menu-list-box > li.on > a{color:#0c6fb6}
.gnb-wrap .gnb-menu-list-box > li > div > ul{padding:0}
.gnb-wrap .gnb-menu-list-box > li > div > ul > li > a{display:block;line-height:1.85rem;font-size:.8rem;text-align:center}
.gnb-wrap .gnb-menu-list-box > li.on > div > ul > li > a{color:#fff}
.gnb-wrap .gnb-menu-list-box > li.on > div > ul > li > a:hover > span{display:inline-block;line-height:30px;position:relative;border-bottom:1px solid #fff;transition: border 1s ease-in}
 

.gnb-wrap .btn-menu{position:absolute;right:0;top:1.5rem}
.gnb-wrap .btn-menu a{display:block;width:2.7rem;height:2.5rem;background:url('../images/hd-menu.png') center center no-repeat;border:1px solid #0c6fb6;box-sizing:border-box;font-size:0}

.gnb-wrap .gnb-menu-bg{background:url('../images/hd-bg-03.png') left bottom repeat-x;height:0;overflow:hidden;box-sizing:border-box;position:absolute;left:0;top:5.16rem;right:0;width:100%;}
.gnb-wrap .gnb-menu-bg .inner{width:64rem;position:relative;margin:0 auto;height:100%;display:none}
.gnb-wrap .gnb-menu-bg.open .inner{display:block;transition: display 1s fadein}


.footer-wrap{background:#2c384b;padding-top:2.5rem;box-sizing:border-box;height:11.5rem;}
.footer-wrap .ft-logo{position:absolute;left:0;top:0;background:url('../images/ft-logo.png') left top no-repeat;font-size:0;height:3.75rem;width:10rem;}
.footer-wrap .base-inner{padding-left:14.5rem;box-sizing:border-box}
.footer-wrap .ft-banner{position:absolute;right:0;top:4.5rem;}
.footer-wrap .ft-banner li{float:left;padding:0 .9rem;position:relative;}
.footer-wrap .ft-banner li:after{position:absolute;left:0;top:0;bottom:0;height:100%;width:1px;background:#fff;content:'';display:block;}
.footer-wrap .ft-banner a{;width:1.5rem;display:block;height:1.25rem;font-size:0}
.footer-wrap .ft-banner li:first-child:after{width:0}
.footer-wrap .ft-banner .sns-blog{background:url('../images/ft-sns-blog.png') center center no-repeat;}
.footer-wrap .ft-banner .sns-ins{background:url('../images/ft-sns-ins.png') center center no-repeat;}
.footer-wrap .ft-banner .sns-face{background:url('../images/ft-sns-face.png') center center no-repeat;}

.footer-wrap .ft-site-box{position:absolute;right:0;top:-.5rem}
.footer-wrap .ft-site-box select{background:#4a5b75;border:none;color:#fff;height:2.5rem;line-height:2.5rem;font-size:.8rem;width:13.5rem}

.footer-wrap .ft-menu-box ul{margin-left:-1rem}
.footer-wrap .ft-menu-box a{color:#fff;font-size:1rem;font-weight:400;display:block;height:1rem}
.footer-wrap .ft-menu-box li{float:left;position:relative;padding:0 1rem;line-height:1}
.footer-wrap .ft-menu-box li:after{position:absolute;left:0;top:0;bottom:0;height:100%;width:1px;background:#fff;content:'';display:block;}
.footer-wrap .ft-menu-box li:first-child:after{width:0}
.footer-wrap .ft-menu-box li.on a{color:#00aeff}

.footer-wrap .sec-address{color:#fff;line-height:1.5rem;padding-top:1.5rem}

.background-cover {
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;
}

#container{position:relative;margin:0 auto}
#container:after, #header:after, #container .bg-snb:after{content:"";display:block;clear:both;}

 
/************************************
**  txt ellipsis  css Start
**************************************/
.ellipsis {  text-overflow: ellipsis;  -o-text-overflow: ellipsis;  overflow: hidden; white-space: nowrap;  word-wrap: normal !important;  display: block; }
.ellipsis_inline { overflow:hidden;white-space:nowrap;text-overflow:ellipsis;display:inline-block }
.ellipsis-h2{white-space: normal; line-height: 1.2; height: 2.4em; text-align: left; word-wrap: break-word; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; }
.ellipsis-h3{white-space: normal; line-height: 1.2; height: 3.6em; text-align: left; word-wrap: break-word; display: -webkit-box; -webkit-line-clamp: 3; -webkit-box-orient: vertical; }
/************************************
**   txt ellipsis css End
**************************************